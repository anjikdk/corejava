package com.amazon;

import java.util.List;

public class SumExample {

	public static void main(String[] args) {
		
		
	}
	
	public static void sum(List<Integer> list)
	{
		for(int i=0; i<list.size(); i++)
		{
			if(i <= list.size())
			{
				if(list.get(i) < list.get(i+1))
				{
					int k = list.get(i)+list.get(i+1);
					list.add(i, k);
				}
			}
		}
	}
}
